<div>
    @model PortfolioVM
    <form method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label asp-for="Title">Title</label>
            <input asp-for="Title" placeholder="Enter title">
            <span asp-validation-for="Title" text-danger></span>
        </div>
        <div class="form-group">
            <label asp-for="SubTitle">SubTitle</label>
            <input asp-for="SubTitle" placeholder="Enter subtitle">
            <span asp-validation-for="SubTitle" text-danger></span>
        </div>
        <div class="form-group">
            <label asp-for="ImgFile">Title</label>
            <input asp-for="ImgFile" placeholder="choose image">
            <span asp-validation-for="ImgFile" text-danger></span>
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>
